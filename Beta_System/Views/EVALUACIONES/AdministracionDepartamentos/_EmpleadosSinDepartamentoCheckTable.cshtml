@model List<Beta_System.Models.C_nomina_empleados>

@if (Model.Count() > 0)
{
    <table class="table table-sm table-condensed small table-bordered table-striped" id="datatable_empleados_sin_depto">
        <thead>
            <tr class="bg-beta text-center">
                <th>Seleccionar</th>
                <th>Nombre</th>
                <th>Apellidos</th>
                <th>Puesto</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td class="text-center">
                        <input type="checkbox" value="@item.Empleado_id" class="id_empelado_departamento_new" />
                    </td>
                    <td>@item.Nombres</td>
                    <td>@item.Apellido_paterno @item.Apellido_materno</td>
                    <td>@item.C_nomina_puestos.nombre_puesto</td>
                </tr>
            }
        </tbody>
    </table>
}