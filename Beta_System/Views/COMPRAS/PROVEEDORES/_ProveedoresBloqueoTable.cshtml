@model IEnumerable<Beta_System.Models.C_compras_proveedores_bloqueos>
@{
    List<int> permisos = Session["PermisosModulo"] as List<int>;
}
<table class="table table-striped table-bordered" id="datatable_proveedores_bloqueo" style="width:100%">
    <thead>
        <tr class="bg-beta text-center">
            <th>Motivo</th>
            <th>Fecha de registro</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="text-center">
                <td>@item.motivo</td>
                <td>@item.fecha_registro</td>
            </tr>
        }
    </tbody>
</table>