@model IEnumerable<Beta_System.Models.ExistenciaAlmacenArticulo>

<table id="tabla_almacen_articulo_existencia" class="table table-striped table-bordered table-sm small">
    <thead>
        <tr class="bg-beta text-center">
            <th>Almacen</th>
            <th>Existencia</th>
            <th>Tránsito</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="text-center">
                <td>@item.almacen</td>
                <td>@item.existencia.ToString("N2")</td>
                <td>@item.transito.ToString("N2")</td>
            </tr>
        }
    </tbody>
</table>
