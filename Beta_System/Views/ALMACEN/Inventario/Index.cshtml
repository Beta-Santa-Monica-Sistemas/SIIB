<script>
    $(window).on("load", function () {
        ConsultarAlmacenesUsuarioSelect(0);
        CaracteresControl();
        ConsultarTiposArticulosSelect();
        $('.fecha_inicio_input').val(last_week);

        //ACEPTA SOLO NUMEROS EL DIV DE CAPTURA DE MOVIMIENTOS
        $(".divEditable").keypress(function (e) {
            if (isNaN(String.fromCharCode(e.which))) e.preventDefault();
        });

        ConsultarInventarioArticulos();
        const todos_option = $('<option>', { value: '0', text: 'Selecciona un establo' });
        $("#id_almacen_g_ajuste_inventario").prepend(todos_option);

        $("#mes_inicio_dif").val(last_six_month);
        $("#mes_fin_dif").val(mes_actual);

    });

    $("#clave_nombre").autocomplete({
        source: function (request, response) {
            $.ajax({
                type: "POST",
                async: false,
                url: "../ALMACEN/ConsultarArticulosQuickView",
                data: {
                    clave_nombre: $("#clave_nombre").val(),
                    id_tipo_articulos: 0
                },
                success: function (data) {
                    var data2 = $.parseJSON(data);
                    response($.map(data2, function (item) {
                        return { label: item.nombre_articulo + " | " + item.clave, value: item.nombre_articulo + " | " + item.clave };
                    }))
                },
                error: function (xhr, status, error) {
                    alert(error);
                }
            });
        }
    });

    $('#clave_nombre').keyup(function () {
        if ($("#clave_nombre").val() != '') {
            var clave_nombre = $("#clave_nombre").val().split('|')[1].trim();
            ConsultarInformacionArticuloAjuste(clave_nombre, 0);
        }
        else {
            $(".inputs_ajuste").val("");
        }
    });

    $("#ui-id-1").on('click', function (event) {
        if ($("#clave_nombre").val() != '') {
            var clave_nombre = $("#clave_nombre").val().split('|')[1].trim();
            ConsultarInformacionArticuloAjuste(clave_nombre, 0);
        }
        else if (event.keyCode === 13 || event.keyCode === 9) {
            iziToast.warning({
                title: 'Aviso',
                message: 'Ingresa el nombre o la clave de articulo para buscar.',
            });
        }
        else {
            $(".inputs_ajuste").val("");
        }
    });

</script>


