@model IEnumerable<Beta_System.Models.C_modulos_sub_permisos_usuarios>
<table class="table table-striped table-bordered">
    <thead>
        <tr class="bg-beta">
            <th>PERMISO</th>
            <th>ASIGNADO</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.nombre_permiso</td>
                <td><input type="checkbox" id="@item.id_submodulo_permiso_usuario" class="form-control check_submodulo" style="height:20px;" /></td>
            </tr>
        }
    </tbody>
    </table>