@model IEnumerable<Beta_System.Models.C_compras_ordenes_ubicaciones_entrega>

<table class="table table-striped table-bordered">
    <thead>
        <tr class="bg-beta text-center">
            <th>Ubicacion</th>
            <th>Asignado</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.nombre_ubicacion</td>
                <td>
                    <input class="form-control check_almacen" type="checkbox" id="@item.id_compras_ubicacion_entrega" style="height:20px;" />
                </td>
            </tr>
        }
    </tbody>
</table>
