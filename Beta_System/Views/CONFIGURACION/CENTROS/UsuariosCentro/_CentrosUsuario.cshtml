@model IEnumerable<Beta_System.Models.C_centros_g>
@{
    var centros = Session["usuarios_centors"] as List<Beta_System.Models.C_usuarios_centros>;
}

<table class="table table-striped table-bordered">
    <thead>
        <tr class="bg-beta text-center">
            <th>Siglas</th>
            <th>Centos</th>
            <th>Asignado</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.siglas</td>
                <td>
                    @item.nombre_centro
                </td>
                <td>
                    <input class="form-control check_centro" type="checkbox" id="@item.id_centro_g" style="height:20px;" />
                </td>
            </tr>
        }
    </tbody>
</table>
