@model IEnumerable<Beta_System.Models.C_nomina_empleados_solicitudes>

<table class="table table-striped table-bordered" id="tabla_solicitud_empleado">
    <thead>
        <tr class="bg-beta text-center">
            <th>Estatus</th>
            <th>Nombre</th>
            <th>Puesto</th>
            <th>RFC</th>
            <th>Seguro social</th>
            <th>Fecha de registro</th>
            <th>Mostrar</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td class="text-center" style="color:@item.C_nomina_empleados_solicitudes_status.color"><strong>@item.C_nomina_empleados_solicitudes_status.nombre_status</strong></td>
                <td>@item.Nombre_completo</td>
                <td>@item.C_nomina_puestos.nombre_puesto</td>
                <td>@item.RFC</td>
                <td>@item.Reg_imss</td>
                <td>@Convert.ToDateTime(item.fecha_registro).ToShortDateString()</td>
                <td class="text-center">
                    <button class="btn btn_beta_icon" onclick="MostrarSolicitudPreRegistroEmpleadosNomina(@item.id_empleado_solicitud);"><i class="fa fa-eye"></i></button>
                </td>
            </tr>
        }
    </tbody>
</table>
