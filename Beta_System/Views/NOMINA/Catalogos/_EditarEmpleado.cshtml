                <div class="col-md-12"><hr /></div>
                <div class="col-md-3">
                    <strong>Nivel de escolaridad</strong>
                    <select class="form-control" id="selectNivelEscolaridad">
                        @{ Html.RenderAction("ConsultarEscolaridadSelect", "CATALOGOS"); }
                    </select>
                </div>
                <div class="col-md-9 p-3 text-right">
                    <button class="btn btn_beta" onclick="NuevoRegistroFormacionAcademica(2);" id="btn_formacion_academica"><i class="fa fa-check" id="i_formacion_academica"></i>Nuevo registro</button>
                </div>

                <div class="col-md-12"><br /></div>

                <div id="div_registro_academico">
                    <div class="col-md-12"><h2>Nuevo registro</h2></div>
                    <label id="formacion_empleado_g" style="display:none"></label>

                    <div class="col-md-12"><hr /></div>

                    <div class="col-md-6">
                        <div class="col-md-12 regTitulo">
                            <strong>Titulo/Carrera</strong>
                            <select class="form-control" id="selectForma_titulo" onchange="ValidacionTituloCarrera(1)">
                                <option value="-1">Nuevo registro</option>
                                @{ Html.RenderAction("ConsultarCarreraSelect", "CATALOGOS"); }
                            </select>
                        </div>
                        <div class="col-md-11 regCarrera">
                            <strong>Nuevo Titulo/Carrera</strong>
                            <input type="text" class="form-control" id="inputForma_titulo" />
                        </div>
                        <div class="col-md-1 p-3 regCarrera">
                            <button class="btn btn_beta_danger" onclick="ValidacionTituloCarrera(2)"><i class="fa fa-remove"></i></button>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="col-md-12 regInstitucion">
                            <strong>Institucion educativa</strong>
                            <select class="form-control" id="selectForma_institucion" onchange="ValidacionInstitucion(1)">
                                <option value="-1">Nuevo registro</option>
                                @{ Html.RenderAction("ConsultarInstitucionSelect", "CATALOGOS"); }
                            </select>
                        </div>
                        <div class="col-md-11 regEducativa">
                            <strong>Nueva institucion educativa</strong>
                            <input type="text" class="form-control" id="inputForma_institucion" />
                        </div>
                        <div class="col-md-1 p-3 regEducativa">
                            <button class="btn btn_beta_danger" onclick="ValidacionInstitucion(2)"><i class="fa fa-remove"></i></button>
                        </div>
                    </div>
                    <div class="col-md-12 p-2">
                        <div class="col-md-2">
                            <strong>Estatus</strong>
                            <select class="form-control" id="selectForma_estatus">
                                @{ Html.RenderAction("ConsultarEscolaridadStatusSelect", "CATALOGOS"); }
                            </select>
                        </div>
                        <div class="col-md-2">
                            <strong>Año de termino</strong>
                            <input class="form-control solo-numeros-spunto" id="inputForma_anio" />
                        </div>
                        <div class="col-md-3">
                            <strong>Numero de Cedula</strong>
                            <input type="text" class="form-control solo-numeros-spunto" id="numero_cedula" />
                        </div>
                        <div class="col-md-3">
                            <strong>Path Cedula</strong>
                            <input class="form-control" type="file" id="uploadForma_pdf" accept="application/pdf" onchange="MostrarBotonConstanciaFiscal();">
                        </div>
                        <div class="col-md-1 p-3 text-right">
                            <button class="btn btn_beta" onclick="ConfirmacionFormacionAcademica();"><i class="fa fa-check"></i> Confirmar</button>
                        </div>
                    </div>
                </div>

                <div class="col-md-12 table-responsive" id="div_table_historico_formacion_academica"></div>
            </div>
        </div>
    </div>
