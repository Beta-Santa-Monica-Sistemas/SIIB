@model IEnumerable<Beta_System.Models.ReporteCumplimientoEnviosLecheData>

<table class="table table-striped table-bordered datatable_cumplimiento_leche">
    <thead>
        <tr class="text-center bg-beta">
            <th>Cliente</th>
            <th>Destino</th>
            <th>Establo</th>
            <th>Ppto</th>
            <th>Real</th>
            <th>Cumplimiento</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="text-center">
                <td>@item.Cliente</td>
                <td>@item.Destino</td>
                <td>@item.Establo</td>
                <td>@item.Ppto_envio</td>
                <td>@item.Real_envio</td>
                @if (@item.Cumplimiento_envio > 100)
                {
                    <td style="color:green">
                        <strong>@item.Cumplimiento_envio%</strong>
                    </td>
                }
                else
                {
                    <td style="color:red">
                        <strong>@item.Cumplimiento_envio%</strong>
                    </td>
                }
            </tr>
        }
    </tbody>
</table>


