@model List<List<string>>

<table class="table table-striped table-bordered">
    <thead>
        <tr class="bg-beta text-center">
            <th>Articulo forraje</th>
            <th>Ingrediente tracker</th>
            <th>Eliminar</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="text-center">
                <td>@item[1]</td>
                <td class="text-left">@item[2]</td>
                <td class="text-left">
                    <button class="btn btn_beta_danger" onclick="EliminarAsociacionArticuloIngrediente(@item[0]);" ><i class="fa fa-remove"></i></button>
                </td>
            </tr>
        }
    </tbody>
</table>